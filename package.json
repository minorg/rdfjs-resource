{
  "dependencies": {
    "@rdfjs/data-model": "~2.1.1",
    "@rdfjs/term-set": "~2.0.3",
    "@rdfjs/types": "~2.0.1",
    "@types/rdfjs__data-model": "~2.0.9",
    "@types/rdfjs__term-set": "~2.0.9",
    "purify-ts": "~2.1.4"
  },
  "description": "Resource abstraction over RDF/JS Datasets",
  "devDependencies": {
    "@biomejs/biome": "2.3.10",
    "@tpluscode/rdf-ns-builders": "~4.3.0",
    "@tsconfig/strictest": "~2.0.8",
    "@types/n3": "~1.26.1",
    "@vitest/coverage-v8": "~4.0.18",
    "housemd": "0.1.3",
    "n3": "~1.26.0",
    "rimraf": "~6.0.1",
    "typescript": "5.9.3",
    "vitest": "~4.0.18"
  },
  "files": [
    "dist/*.d.ts",
    "dist/*.js"
  ],
  "license": "Apache-2.0",
  "main": "dist/index.js",
  "name": "rdfjs-resource",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minorg/rdfjs-resource"
  },
  "scripts": {
    "build": "tsc -b",
    "build:tests": "tsc -b -p __tests__",
    "check": "biome check",
    "check:write": "biome check --write",
    "check:write:unsafe": "biome check --write --unsafe",
    "clean": "rimraf dist tsconfig.tsbuildinfo",
    "dev": "tsc -w --preserveWatchOutput",
    "dev:tests": "tsc -w --preserveWatchOutput -p __tests__",
    "test": "biome check && vitest run",
    "test:coverage": "biome check && vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "type": "module",
  "types": "dist/index.d.ts",
  "version": "2.0.1"
}
