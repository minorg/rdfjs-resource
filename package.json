{
  "dependencies": {
    "@rdfjs/term-set": "^2.0.3",
    "@rdfjs/types": "^1.1.2",
    "@types/rdfjs__term-set": "^2.0.9",
    "purify-ts": "~2.1.0",
    "rdf-literal": "^1.3.2"
  },
  "description": "Resource abstraction over RDF/JS Datasets",
  "devDependencies": {
    "@biomejs/biome": "1.9.4",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "@tsconfig/strictest": "^2.0.5",
    "@types/n3": "^1.26.0",
    "@vitest/coverage-v8": "^2.0.5",
    "housemd": "^0.1.3",
    "n3": "^1.26.0",
    "rimraf": "^6.0.1",
    "typescript": "5.8.2",
    "vitest": "^2.0.5"
  },
  "files": ["*.d.ts", "*.js"],
  "license": "Apache-2.0",
  "main": "index.js",
  "name": "rdfjs-resource",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minorg/rdfjs-resource"
  },
  "scripts": {
    "build": "tsc -b",
    "check": "biome check",
    "check:write": "biome check --write",
    "check:write:unsafe": "biome check --write --unsafe",
    "clean": "rimraf *.d.ts* *.js *.js.map __tests__/*.d.ts* __tests__/*.js __tests__/*.js.map tsconfig.tsbuildinfo",
    "dev": "tsc -w --preserveWatchOutput",
    "test": "biome check && vitest run",
    "test:coverage": "biome check && vitest run --coverage",
    "test:watch": "vitest watch"
  },
  "type": "module",
  "types": "index.d.ts",
  "version": "1.0.24"
}
