{
  "dependencies": {
    "@rdfjs/term-set": "^1.1.0",
    "@rdfjs/types": "^1.1.0",
    "@tpluscode/rdf-ns-builders": "^4.3.0",
    "@types/rdfjs__term-set": "^1.0.2",
    "purify-ts": "~2.1.0",
    "rdf-literal": "^1.3.2"
  },
  "devDependencies": {
    "@biomejs/biome": "1.8.3",
    "@tsconfig/strictest": "^2.0.5",
    "@types/n3": "^1.16.5",
    "@vitest/coverage-v8": "^2.0.5",
    "n3": "^1.20.4",
    "rimraf": "^6.0.1",
    "typescript": "~5.5",
    "vitest": "^2.0.5"
  },
  "engines": {
    "node": ">=20"
  },
  "main": "index.js",
  "files": ["*.d.ts", "*.js"],
  "license": "Apache-2.0",
  "name": "rdfjs-resource",
  "scripts": {
    "build": "tsc -b",
    "check": "biome check",
    "check:write": "biome check --write",
    "check:write:unsafe": "biome check --write --unsafe",
    "clean": "rimraf *.d.ts* *.js *.js.map __tests__/*.d.ts* __tests__/*.js __tests__/*.js.map tsconfig.tsbuildinfo",
    "format": "biome format",
    "format:write": "biome format --write",
    "format:write:unsafe": "biome format --write --unsafe",
    "rebuild": "run-s clean build",
    "lint": "biome lint",
    "lint:write": "biome lint --write",
    "lint:write:unsafe": "biome lint --write --unsafe",
    "test": "biome check && vitest run",
    "test:coverage": "biome check && vitest run --coverage",
    "test:watch": "vitest watch",
    "watch": "tsc -w --preserveWatchOutput"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/minorg/rdfjs-resource"
  },
  "type": "module",
  "types": "index.d.ts",
  "version": "1.0.0"
}
